package january23;


public class Main {
	
	
	public class SingleLinkedList<T>{
		public Node<T> head = null;
	    
	    public class Node<T> {
	        T data;
	        Node<T> next = null; //∆˜¿Œ≈Õ 
	        
	        public Node(T data) {
	            this.data = data;
	        }
	        		
		public void addNode (T data) {
			if(head == null) {
				head = new Node<T>(data);
			} else {
				Node<T> node = this.head;
				while(node.next != null) {
					node = node.next ;
				}
				node.next = new Node<T>(data);
			}
		}
	}
	public static void main(String[] args) {
		SingleLinkedList<Integer> a1 = new SingleLinkedList<Integer>();
		.addNode(1);
	}
}
